{
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "headers": [
    {
      "source": "/main.py",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=59"
        },
        {
          "key":"Vercel-CDN-Cache-Control",
          "value": "max-age=3600"
        }
      ]
    }
  ]
  }